    <text>
        <body>
            <entry xml:id="cat-n">
                <form type="lemma">
                    <orth>cat</orth>
                    <orth xml:lang="sl">ma훾ka</orth>
                </form>
                <gramGrp>
                    <pos norm="NOUN">noun</pos>
                </gramGrp>
                <sense n="1" xml:id="cat-n-1">
                    <def>a type of animal</def>
                    <def xml:lang="sl">vrsta 탑ivali</def>
                </sense>
                <sense n="2" xml:id="cat-n-2">
                    <def>an attractive woman</def>
                    <def xml:lang="sl">privla훾na 탑enska</def>
                </sense>
            </entry>
            <entry xml:id="cat-v">
                <form type="lemma">
                    <orth>cat</orth>
                </form>
                <gramGrp>
                    <pos norm="VERB">verb</pos>
                </gramGrp>
                <sense n="1" xml:id="cat-v-1">
                    <def>print contents of a computer file</def>
                </sense>
                <sense n="2" xml:id="cat-v-2">
                    <def>raise (an anchor) from the surface of the water to the cathead</def>
                </sense>
            </entry>
        </body>
